<%= form_for @actor do |f| %>
    <%= f.label :name %><br />
    <%= f.text_field :name %><br />

    <%= f.label :age %><br />
    <%= f.number_field :age %><br />

    <%= f.label :bio %><br />
    <%= f.text_area :bio %><br />
    <%=f.collection_select :movie_ids, Movie.all, :id, :title,{:prompt => "Select a movie "}%><br>

    
  <%= f.submit%>
<% end %>